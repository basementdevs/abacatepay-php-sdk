{
  "name": "basement/abacatepay-php-sdk",
  "description": "AbacatePay PHP SDK Client",
  "version": "1.0.0",
  "type": "library",
  "autoload": {
    "psr-4": {
      "Basement\\AbacatePay\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.3",
    "guzzlehttp/guzzle": "^7.10.0",
    "symfony/http-foundation": "^v7.3.4"
  },
  "require-dev": {
    "pestphp/pest": "^4.1",
    "laravel/pint": "^v1.25.1",
    "rector/rector": "^2.2",
    "mockery/mockery": "^1.6"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
